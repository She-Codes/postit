<% if logged_in? %>
  <div class="span0 well text-center">
    <%= link_to path, method: 'post' do %>
      <i class="icon-arrow-up"></i>
    <% end %>
    <br>
    <%= object.total_votes %>
    <br>
    <%= link_to path, method: 'post' do %>
      <i class="icon-arrow-down"></i>
    <% end %>
  </div><!--end .well-->
<% end %>